#################################################################
# Service

# Native service
on property:ro.minios.bootcomplete=1
    start qcom-post-boot
    write /sys/class/i2c-dev/i2c-6/device/6-0072/enable_irq 1

service appops /sbin/appops
    class main
    user root
    group root
    seclabel u:r:minios_services:s0

service sensorservice /system/bin/sensorservice
    class late_start
    user system
    group system input root
    disabled
    seclabel u:r:minios_services:s0

service minios_isensorservice /sbin/minios_isensorservice
    class late_start
    user root
    group system input root
    disabled
    seclabel u:r:minios_services:s0

# LGE service
service mtsd /vendor/bin/mtsd
    class late_start
    oneshot
    socket mtsd.port stream 660 system system
    seclabel u:r:minios_services:s0

on property:sys.mts.forcemts=1
    start mtsd

on property:sys.mts.forcemts=0
    stop mtsd


# MiniOS service
on init
    chmod 0777 /vendor/bin/dhcpcd/dhcpcd-run-hooks
    chmod 0777 /vendor/bin/dhcpcd/dhcpcd-hooks/20-dns.conf
    chmod 0777 /vendor/bin/dhcpcd/dhcpcd-hooks/95-configured
    chown dhcp dhcp /vendor/bin/dhcpcd/dhcpcd-run-hooks
    chown dhcp dhcp /vendor/bin/dhcpcd/dhcpcd-hooks/20-dns.conf
    chown dhcp dhcp /vendor/bin/dhcpcd/dhcpcd-hooks/95-configured

service minios_per /sbin/minios_permissionservice
    class late_start
    user root
    group root
    disabled
    seclabel u:r:minios_services:s0

