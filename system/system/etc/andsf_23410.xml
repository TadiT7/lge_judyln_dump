        </MAPCON_WIFI_O_WIFIBAD_LTE_O_LTEBAD>

<!-- WiFi Not available && LTE available case -->
        <MAPCON_WIFI_X_LTE_O>
          <APN>IMS</APN>
          <RoutingCriteria>
            <Node>
              <OPSpecificRANValidityCondition>
                <ThresholdConditions>
