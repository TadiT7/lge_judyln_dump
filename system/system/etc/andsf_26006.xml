                    <WiFiAvailable>1</WiFiAvailable>
                    <WiFiRSSILow>-80</WiFiRSSILow>
                  </WiFiThreshold>
                </ThresholdConditions>
              </OPSpecificRANValidityCondition>
                 <CallConditions>
                  <Node>
                      <CallType>0</CallType>
                  </Node>
                 </CallConditions>
            </Node>
          </RoutingCriteria>
          <IsAPIOverride>1</IsAPIOverride>
          <RulePriority>5</RulePriority>
